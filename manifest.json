{
    "name": "Chungus TTS",
    "description": "Reads highlighted text in your browser!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup/base.html"
    },
    "icons": {
      "16": "images/ChungusTTSIcon16.png",
      "128": "images/ChungusTTSIconV1.png"
    },
    "content-scripts": [
      {
        "js": ["scripts/main.js"],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }